{
  "name": "2045_qn-backend-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build": "tsc && cp -r generated dist/generated",
    "test": "jest",
    "start": "npm run build && node dist/index.js",
    "start:dev": "nodemon",
    "start:staging": "nodemon -r dotenv/config --exec ts-node index.ts dotenv_config_path=.env.staging",
    "start:production": "nodemon -r dotenv/config --exec ts-node index.ts dotenv_config_path=.env.production",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write '**/*.{ts,js,json}'",
    "generate": "npm run generate-main && npm run generate-retool",
    "generate-main": "npx prisma generate --schema=./prisma/schema.prisma",
    "generate-retool": "npx prisma generate --schema=./prisma-retool/schema.prisma",
    "docker:build": "docker build -t 2045_qn-backend-app .",
    "docker:run": "docker run -d -p 3009:3009 --name 2045_qn-backend-app --env-file .env 2045_qn-backend-app",
    "seed:dev": "npx ts-node ./prisma/seeders/index.ts",
    "seed:staging": "npx ts-node -r dotenv/config ./prisma/seeders/index.ts dotenv_config_path=.env.staging",
    "seed:production": "npx ts-node -r dotenv/config ./prisma/seeders/index.ts dotenv_config_path=.env.production"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.1",
    "prisma": "^5.15.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@azure/identity": "^4.2.0",
    "@azure/keyvault-secrets": "^4.8.0",
    "@prisma/client": "^5.15.0",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "joi": "^17.13.1",
    "moment": "^2.30.1",
    "newrelic": "^11.22.0",
    "winston": "^3.13.0"
  },
  "engines": {
    "node": "16.16.0"
  }
}
